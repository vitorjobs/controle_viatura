{
  "name": "setupnodets",
  "version": "1.0.0",
  "description": "Setup padrÃ£o",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "start:open": "nodemon --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node src/server.ts' -e ts prisma:studio",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTests",
    "test:push": "npm run test --coverage",
    "build": "tsc",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "start:docker": "concurrently \"npm run start:dev\" \"npm run prisma:studio\"",
    "start:teste": "concurrently  \"npm run prisma:migrate\" \"npm run start:dev\" \"npm run prisma:studio\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "add": "^2.0.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/uuidv4": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuidv4": "^6.2.13",
    "pg": "8.10.0"
  }
}
